<Window x:Class="Case_Assistant.CreateDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Case_Assistant"
        mc:Ignorable="d"
        Title="CreateDataWindow" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Open the SR in the CritSit portal, open all tabs and press Control+A and Control+C, then paste it below:" VerticalAlignment="Top"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="707,389,0,0" VerticalAlignment="Top" Width="75" Click="Save"/>
        <TextBox x:Name="DataBox" Text="" HorizontalAlignment="Left" Height="358" Margin="10,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="772" AcceptsReturn="True"/>

    </Grid>
</Window>
